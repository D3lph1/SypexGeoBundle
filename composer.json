{
    "name": "yamilovs/sypex-geo-bundle",
    "description": "An adaptation of SypexGeo library to Symfony Bundle system",
    "keywords": [
        "sypexgeo", "position", "ip location", "geoip"
    ],
    "homepage": "https://github.com/yamilovs/SypexGeoBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yamilov Stepan",
            "email": "yamilovs@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Yamilovs\\Bundle\\SypexGeoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yamilovs\\Bundle\\SypexGeoBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "symfony/framework-bundle": "^6.0",
        "yamilovs/sypex-geo": "patch-1#e441244ef17bd6c602d11a2b1c3de7c04fda46ff"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3|^9.5",
        "symfony/console": "^6.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gam6itko/SypexGeo.git"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
